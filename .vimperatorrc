"2.1 (created: 2009/05/21 13:13:06)

set runtimepath=~/.vimperator
" vim: set ft=vimperator:
set ds=yubnub
set go=n

javascript <<EOF
(function(){
  var feedPanel = document.createElement("statusbarpanel");
  feedPanel.setAttribute("id", "feed-panel-clone");
  feedPanel.appendChild(document.getElementById("feed-button"));
  feedPanel.firstChild.setAttribute("style", "padding: 0; max-height: 16px;");
  document.getElementById("status-bar")
  .insertBefore(feedPanel, document.getElementById("security-button"));
})();
EOF

"feedSomeKeys.js
"autocmd LocationChange .* :fmapc
"autocmd LocationChange mail\.google\.com/mail :fmap -depth 4 c / j k n p o u e x s r a y # [ ] z ? gi gs gt gd ga gc
"autocmd LocationChange www\.google\.com/reader :fmap! -vkey j k n p m s t v y cA A r S N P X O gh ga gs gt gu u / ? <Space>

autocmd LocationChange .* js modes.passAllKeys = /mail\.google\.com/.test(buffer.URL)
autocmd LocationChange .* js modes.passAllKeys = /www\.google\.com\/reader/.test(buffer.URL)
