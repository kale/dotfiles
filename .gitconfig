[alias]
  up = pull origin
  st = status
  di = diff
  co = checkout
  ci = commit
  br = branch
  sta = stash
  lg = log -p
[core]
  pager = less
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
